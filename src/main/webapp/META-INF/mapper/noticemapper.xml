<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goodee.dao.NoticeDAO">
	
	<select id="getList" resultType="NoticeVO">
		select * from notice
	</select>
	
	<select id="getContents" resultType="NoticeVO">
		select title, content from notice where id = #{id}
	</select>
</mapper>

